{
  "name": "Repository Enrichment - 5min",
  "nodes": [
    {
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "minutes",
              "minutesInterval": 5
            }
          ]
        }
      },
      "id": "1",
      "name": "Schedule Trigger",
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1.1,
      "position": [240, 300]
    },
    {
      "parameters": {
        "method": "GET",
        "url": "https://yourdomain.com/api/automation/status",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth"
      },
      "id": "2",
      "name": "Check Status",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [460, 300],
      "credentials": {
        "httpHeaderAuth": {
          "id": "spy-api-key",
          "name": "Spy API Key"
        }
      }
    },
    {
      "parameters": {
        "conditions": {
          "boolean": [
            {
              "value1": "={{$json.nextActions.shouldRunEnrichment}}",
              "value2": true
            }
          ]
        }
      },
      "id": "3",
      "name": "Should Run Enrichment?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [680, 300]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "https://yourdomain.com/api/automation/enrich",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "options": {
          "timeout": 300000,
          "retry": {
            "enabled": true,
            "maxTries": 2
          }
        }
      },
      "id": "4",
      "name": "Trigger Enrichment",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [900, 200],
      "credentials": {
        "httpHeaderAuth": {
          "id": "spy-api-key",
          "name": "Spy API Key"
        }
      }
    },
    {
      "parameters": {
        "message": "‚ÑπÔ∏è No repositories need enrichment at this time.\n\nüìä Current Status:\n- Total Repos: {{$('Check Status').item.json.statistics.total}}\n- Enriched: {{$('Check Status').item.json.statistics.enriched}}\n- Need Enrichment: {{$('Check Status').item.json.nextActions.enrichmentsNeeded}}"
      },
      "id": "5",
      "name": "Skip Notification",
      "type": "n8n-nodes-base.noOp",
      "typeVersion": 1,
      "position": [900, 400]
    },
    {
      "parameters": {
        "conditions": {
          "boolean": [
            {
              "value1": "={{$json.success}}",
              "value2": true
            }
          ]
        }
      },
      "id": "6",
      "name": "Check Enrichment Success",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [1120, 200]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "https://yourdomain.com/api/revalidate",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendBody": true,
        "contentType": "json",
        "body": "={\"path\": \"/\"}",
        "options": {
          "timeout": 30000
        }
      },
      "id": "9",
      "name": "Revalidate Cache",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [1340, 100],
      "credentials": {
        "httpHeaderAuth": {
          "id": "revalidation-secret",
          "name": "Revalidation Secret"
        }
      }
    },
    {
      "parameters": {
        "message": "‚úÖ Repository enrichment completed!\n\nüìä Results:\n- Total Found: {{$('Trigger Enrichment').item.json.totalFound}}\n- Processed: {{$('Trigger Enrichment').item.json.processed}}\n- Errors: {{$('Trigger Enrichment').item.json.errors}}\n- Timestamp: {{$('Trigger Enrichment').item.json.timestamp}}\n\nüîÑ Cache revalidated successfully!"
      },
      "id": "7",
      "name": "Success Notification",
      "type": "n8n-nodes-base.noOp",
      "typeVersion": 1,
      "position": [1560, 100]
    },
    {
      "parameters": {
        "message": "‚ùå Repository enrichment failed!\n\nüö® Error: {{$('Trigger Enrichment').item.json.error}}\n- Timestamp: {{$('Trigger Enrichment').item.json.timestamp}}"
      },
      "id": "8",
      "name": "Error Notification",
      "type": "n8n-nodes-base.noOp",
      "typeVersion": 1,
      "position": [1340, 300]
    }
  ],
