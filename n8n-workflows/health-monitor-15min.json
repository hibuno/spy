{
  "name": "Health Monitor - 15min",
  "nodes": [
    {
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "minutes",
              "minutesInterval": 15
            }
          ]
        }
      },
      "id": "1",
      "name": "Schedule Trigger",
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1.1,
      "position": [240, 300]
    },
    {
      "parameters": {
        "url": "https://yourdomain.com/api/automation/health",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "X-API-Key",
              "value": "={{$credentials.apiKey}}"
            }
          ]
        },
        "options": {
          "timeout": 30000
        }
      },
      "id": "2",
      "name": "Health Check",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [460, 300],
      "credentials": {
        "httpHeaderAuth": {
          "id": "spy-api-key",
          "name": "Spy API Key"
        }
      }
    },
    {
      "parameters": {
        "conditions": {
          "string": [
            {
              "value1": "={{$json.status}}",
              "value2": "healthy"
            }
          ]
        }
      },
      "id": "3",
      "name": "Is Healthy?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [680, 300]
    },
    {
      "parameters": {
        "message": "üö® SYSTEM HEALTH ALERT!\n\n‚ö†Ô∏è Status: {{$json.status}}\n‚ùå Error: {{$json.error}}\n\nüîß Service Status:\n- Database: {{$json.services.database.status}}\n- GitHub: {{$json.services.github.status}}\n- OpenAI: {{$json.services.openai.status}}\n\nüìã Missing Environment Variables:\n{{$json.environment.missingEnvVars}}\n\nüïê Timestamp: {{$json.timestamp}}\n\nPlease check the system immediately!"
      },
      "id": "4",
      "name": "Health Alert",
      "type": "n8n-nodes-base.noOp",
      "typeVersion": 1,
      "position": [900, 400]
    },
    {
      "parameters": {
        "message": "‚úÖ System health check passed\n\nüìä Status: {{$json.status}}\nüóÑÔ∏è Database: {{$json.services.database.totalRepositories}} repositories\nüîë GitHub Token: {{$json.services.github.hasToken ? 'Configured' : 'Missing'}}\nü§ñ OpenAI: {{$json.services.openai.model}} via {{$json.services.openai.apiUrl}}\n\nüïê {{$json.timestamp}}"
      },
      "id": "5",
      "name": "Health OK Log",
      "type": "n8n-nodes-base.noOp",
      "typeVersion": 1,
      "position": [900, 200]
    }
  ],
  "connections": {
    "Schedule Trigger": {
      "main": [
        [
          {
            "node": "Health Check",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Health Check": {
      "main": [
        [
          {
            "node": "Is Healthy?",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Is Healthy?": {
      "main": [
        [
          {
            "node": "Health OK Log",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Health Alert",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
